{
    "version": 2,
    "builds": [
        {
            "src": "apps/frontend/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/apps/frontend/$1"
        }
    ],
    "env": {
        "VITE_API_URL": "@vite_api_url"
    }
}